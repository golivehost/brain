{
  "name": "golivehost/brain",
  "description": "A comprehensive PHP neural network library with support for various network architectures",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Shubhdeep Singh",
      "homepage": "https://github.com/shubhdeepdev",
      "role": "Developer"
    },
    {
      "name": "Go Live Web Solutions",
      "homepage": "https://golive.host",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "GoLiveHost\\Brain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoLiveHost\\Brain\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "cs-check": "vendor/bin/phpcs --standard=PSR12 src tests",
    "cs-fix": "vendor/bin/phpcbf --standard=PSR12 src tests"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "neural-network",
    "machine-learning",
    "artificial-intelligence",
    "deep-learning",
    "lstm",
    "rnn",
    "gru",
    "liquid-state-machine",
    "php",
    "ai"
  ],
  "support": {
    "issues": "https://github.com/golivehost/brain/issues",
    "source": "https://github.com/golivehost/brain"
  }
}
